art("int","xi","xj")
u.xi = 1
u.xj = 1
assert u.xi == u.xj && u.xi == 1
u.xi++
assert u.xi > u.xj
u.xi--
assert u.xi == u.xj

art("real","xri","xrj")
u.xri = 1
u.xrj = 1
assert u.xri == u.xrj && u.xri == 1
u.xri++
assert u.xri > u.xrj
u.xri--
assert u.xri == u.xrj

art("TEXT","xt1","xt2")
u.xt1 = "Geht"
u.xt2 = "Geht nicht"
assert u.xt1 == "Geht"
assert u.xt1 != u.xt2
u.xt1 += " nicht"
assert u.xt1 == u.xt2

art("GD","xvon","xbis")
u.xvon = "."
u.xbis = "."
assert u.xvon == u.xbis
assert (u.xvon - u.xbis) == 0
u.xbis++
assert u.xvon < u.xbis
u.xvon = u.xvon + 2
assert u.xvon > u.xbis

